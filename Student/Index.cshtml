@model IEnumerable<SampleSecureWeb.Models.Student>

    <h2>Student</h2>
    <table class="table table-stripped">
    <tr>
        <td>Nim</td>
        <td>FullName</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.Nim</td>
        </tr>
    }
    </table>
    